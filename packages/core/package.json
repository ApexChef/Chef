{
  "name": "@chef/core",
  "version": "0.1.0",
  "description": "Core library for Chef - LLM routing, RAG, and pipeline framework",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./llm": {
      "types": "./dist/llm/index.d.ts",
      "import": "./dist/llm/index.js"
    },
    "./rag": {
      "types": "./dist/rag/index.d.ts",
      "import": "./dist/rag/index.js"
    },
    "./pipeline": {
      "types": "./dist/pipeline/index.d.ts",
      "import": "./dist/pipeline/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.3.14",
    "@langchain/core": "^0.3.27",
    "@langchain/langgraph": "^0.2.42",
    "@langchain/ollama": "^0.1.4",
    "chromadb": "^1.9.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}
